generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Ukoly {
  Id      Int      @id @default(autoincrement())
  Nazev   String   // required, proto prostÄ› String
  Datum   DateTime
  Hotovo  Boolean  @default(false)
  Smazano Boolean  @default(false)
}